{
  "name": "discord-music-bot",
  "version": "1.0.0",
  "description": "A simple discord music bot",
  "author": "phxgg",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "preview": "tsc && node .",
    "dev": "concurrently \"tsc -w\" \"nodemon --watch dist dist/index.js\"",
    "deploy": "node scripts/deploy.js",
    "undeploy": "node scripts/undeploy.js",
    "deploy-global": "node scripts/deployGlobal.js",
    "undeploy-global": "node scripts/undeployGlobal.js",
    "ncu:check": "npx npm-check-updates -x @types/node",
    "ncu:update": "npx npm-check-updates -u -x @types/node"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "discord.js",
    "node.js"
  ],
  "_moduleAliases": {
    "@": "./dist"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.5.1",
    "discord-player": "^6.7.1",
    "discord-player-youtubei": "^1.3.4",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "mediaplex": "^0.0.9",
    "winston": "^3.16.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.16.11",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "concurrently": "^9.0.1",
    "eslint": "^9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  }
}
