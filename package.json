{
  "name": "discord-music-bot",
  "version": "1.0.0",
  "description": "A simple discord music bot",
  "author": "phxgg",
  "engines": {
    "node": ">=22"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "preview": "tsc && node .",
    "dev": "concurrently \"tsc -w\" \"nodemon --watch dist dist/index.js\"",
    "deploy": "node scripts/deploy.js",
    "undeploy": "node scripts/undeploy.js",
    "deploy-global": "node scripts/deployGlobal.js",
    "undeploy-global": "node scripts/undeployGlobal.js",
    "ncu:check": "npx npm-check-updates",
    "ncu:update": "npx npm-check-updates -u"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "discord.js",
    "node.js"
  ],
  "_moduleAliases": {
    "@": "./dist"
  },
  "dependencies": {
    "@discord-player/extractor": "^7.1.0",
    "discord-player": "^7.1.0",
    "discord-player-youtubei": "^1.4.2",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "mediaplex": "^1.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.13.8",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  }
}
