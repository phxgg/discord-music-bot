{
  "name": "discord-music-bot",
  "version": "1.0.0",
  "description": "A simple discord music bot",
  "author": "phxgg",
  "engines": {
    "node": ">=20"
  },
  "main": "src/app.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "deploy": "node scripts/deploy.js",
    "undeploy": "node scripts/undeploy.js",
    "deploy-global": "node scripts/deployGlobal.js",
    "undeploy-global": "node scripts/undeployGlobal.js",
    "ncu:check": "npx npm-check-updates -x eslint",
    "ncu:update": "npx npm-check-updates -u -x eslint"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "discord.js",
    "node.js"
  ],
  "dependencies": {
    "@discord-player/extractor": "^4.5.1",
    "discord-player": "^6.7.1",
    "discord-player-youtubei": "^1.2.6",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "mediaplex": "^0.0.9",
    "winston": "^3.14.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "nodemon": "^3.1.4"
  }
}
